##############header_part style-team.css##################

#header_part {
    background: #ffffff;
    box-shadow: 0 20px 20px -30px;
    z-index: 99999999999999999999999999;
    transition: top 0.6s;
}

#header_part nav.navbar {
    padding: 0;
    height: 90px;
}

#header_part nav ul li a.nav-link {
    padding: 0 20px;
    line-height: 90px;
    color: #000000;
    transform: all linear 0.3s;
    position: relative;
    font-family: 'Karla', sans-serif;
    font-size: 18px;
    transition: all ease 0.3s;
}

#header_part nav ul li .nav-link.active:before {
    content: '';
    position: absolute;
    top: 20px;
    right: 0;
    width: 0%;
    height: 3px;
    background: rgb(0, 199, 255);
    transition: all ease 0.3s;
}

#header_part nav ul li .nav-link.active:after {
    content: '';
    position: absolute;
    bottom: 20px;
    left: 0;
    width: 0%;
    height: 3px;
    background: rgb(0, 199, 255);
    transition: all ease 0.3s;
}
#header_part nav ul li .nav-link:before {
    content: '';
    position: absolute;
    top: 20px;
    right: 0;
    width: 0%;
    height: 3px;
    background: rgb(0, 199, 255);
    transition: all ease 0.3s;
}

#header_part nav ul li .nav-link:after {
    content: '';
    position: absolute;
    bottom: 20px;
    left: 0;
    width: 0%;
    height: 3px;
    background: rgb(0, 199, 255);
    transition: all ease 0.4s;
}

#header_part nav ul li .nav-link.active:before,
#header_part nav ul li .nav-link.active:after,
#header_part nav ul li .nav-link:hover:before,
#header_part nav ul li .nav-link:hover:after{
    width: 100%;
}

#header_part nav .navbar-brand img {
    width: 90px;
}

#header_part nav .join_us a {
    margin-left: 30px;
    text-decoration: none;
    background: rgb(0, 199, 255);
    color: #ffffff;
    font-size: 20px;
    padding: 10px 40px;
    border-radius: 5px;
    font-family: 'Karla', sans-serif;
    border: 1px solid #00c7ff;
    transition: all linear 0.3s;
}

#header_part nav .join_us a:hover {
    background: #ffffff;
    color: #00c7ff;
}

